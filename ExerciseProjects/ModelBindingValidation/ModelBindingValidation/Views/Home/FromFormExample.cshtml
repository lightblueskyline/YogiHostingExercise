@{
    ViewData["Title"] = "FromFormExample";
}

<h2>From Form</h2>
<h3>@ViewBag.Message</h3>

<form method="post">
    <div class="form-group">
        <label>Id:</label>
        <input name="Id" class="form-control" />
    </div>
    <div class="form-group">
        <label>Name:</label>
        <input name="Name" class="form-control" />
    </div>
    <button class="btn btn-primary">Submit</button>
</form>
